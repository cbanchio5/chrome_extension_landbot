{
  "manifest_version": 3,
  "name": "Landbot",
  "version": "1.0",
  "description": "Embed external bot alongside Intercom",
  "permissions": ["activeTab", "scripting"],
  "icons": {
  "16": "bot-icon.png",
  "48": "bot-icon.png",
  "128": "bot-icon.png"
},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
